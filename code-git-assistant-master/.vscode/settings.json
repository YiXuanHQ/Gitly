{
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": "explicit"
    },
    "typescript.tsdk": "node_modules/typescript/lib",
    "typescript.enablePromptUseWorkspaceTsdk": true,
    "files.exclude": {
        "out": false,
        "dist": false,
        "node_modules": true
    },
    "search.exclude": {
        "out": true,
        "dist": true,
        "node_modules": true,
        "*.vsix": true
    },
    "files.watcherExclude": {
        "**/node_modules/**": true,
        "**/dist/**": true,
        "**/out/**": true
    }
}